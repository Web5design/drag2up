{
  "name": "drag2up",
  "version": "2.0.0",
  "description": "Drag files onto any site to upload.",
	"background_page": "background.html",
  "permissions": [
    "http://drag2up.appspot.com/",
    "https://api.imgur.com/",
    "http://gist.github.com/",
    "http://pastebin.com/",
    "http://www.imageshack.us/"
  ],
  "page_action": {
    "default_icon": "icon/22.png",
    "default_title": "Uploading files..."
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dragupload.js"],
      "all_frames": true
    }
  ],
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "64": "icon/64.png",
    "128": "icon/128.png",
    "512": "icon/512.png"
  }
}
